{
  "manifest_version": 2,
  "name": "Simple Timer",
  "description": "Simple Timer - That's it!",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/16.png",
      "24": "icon/24.png",
      "32": "icon/32.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon/16.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "128": "icon/128.png"
  }
}